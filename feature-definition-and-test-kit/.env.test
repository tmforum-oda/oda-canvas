# Environment variables for BDD testing
# Copy this file to .env and update with your actual values

# Keycloak configuration (if using identity management tests)
KEYCLOAK_USER=admin
KEYCLOAK_PASSWORD=admin
KEYCLOAK_BASE_URL=http://localhost:8080
KEYCLOAK_REALM=master

# Kubernetes configuration (optional - uses kubectl config by default)
# KUBECONFIG=/path/to/kubeconfig

# Test configuration
TEST_NAMESPACE=components
CANVAS_NAMESPACE=canvas
TEST_TIMEOUT=30000

# PDB Operator configuration
PDB_OPERATOR_NAME=canvas-pdb-management-operator
PDB_OPERATOR_NAMESPACE=canvas