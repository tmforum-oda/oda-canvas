{
  "include": [
    "services/**/*.js",
    "controllers/**/*.js",
    "utils/**/*.js"
  ],
  "exclude": [
    "test/**",
    "node_modules/**",
    "utils/json-schema-ref-parser/**",
    "**/*.test.js"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "check-coverage": true,
  "lines": 80,
  "functions": 80,
  "branches": 70,
  "statements": 80
}